{
    "name": "symfony/framework-standard-edition",
    "license": "MIT",
    "type": "project",
    "description": "The \"Symfony Standard Edition\" distribution. SaxID-Ldap-Proxy",
    "authors": [
        {
            "name": "Moritz Hesse",
            "email": "moritz.hesse@tu-dresden.de"
        },
        {
            "name": "Jan FrÃ¶mberg",
            "email": "jan.froemberg@tu-dresden.de"
        },
        {
            "name": "Norman Walther",
            "email": "norman.walther@tu-dresden.de"
        }
    ],
    "autoload": {
        "psr-4": {
          "SaxidLdapProxyBundle\\": "src/Saxid/SaxidLdapProxyBundle"
      },
      "classmap": [ "app/AppKernel.php", "app/AppCache.php" ]
    },
    "require": {
        "php": ">=5.5",
        "symfony/symfony": "~3.0",
        "incenteev/composer-parameter-handler": "~2.0",
        "sensio/distribution-bundle": "~5.0",
        "sensio/framework-extra-bundle": "^5.0.0",
        "symfony/monolog-bundle": "~3.4",
        "symfony/swiftmailer-bundle": "~3.2",
        "sax_id/simplesamlphp-bundle": "~2.0.0"

    },
    "repositories": [
        {
            "type": "vcs",
            "url":  "git@gitlab.hrz.tu-chemnitz.de:saxid-federated-resource-management/simplesamlphp-bundle.git"
        }
    ],
    "require-dev": {
        "sensio/generator-bundle": "^3.0",
        "symfony/phpunit-bridge": "^3.0"
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-root-package-install": [
            "SymfonyStandard\\Composer::hookRootPackageInstall"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "config": {
        "bin-dir": "bin",
        "platform": {
            "php": "7.2"
        },
        "sort-packages": true
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-var-dir": "var",
        "symfony-bin-dir": "bin",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
            "dev-master": "3.4-dev"
        }
    }
}
